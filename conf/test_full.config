/*
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Nextflow config file for running full-size tests
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
    Defines input files and everything required to run a full size pipeline test.

    Use as follows:
        nextflow run REPO4EU/modulediscovery -profile test_full,<docker/singularity> --outdir <OUTDIR>

----------------------------------------------------------------------------------------
*/

params {
    config_profile_name        = 'Full test profile'
    config_profile_description = 'Full test dataset to check pipeline function'

    // Input data for full size test
    input           = "https://github.com/REPO4EU/modulediscovery_test_data/blob/main/seedfiles/8505C.txt,https://github.com/REPO4EU/modulediscovery_test_data/blob/main/seedfiles/B-CPAP.txt,https://github.com/REPO4EU/modulediscovery_test_data/blob/main/seedfiles/Cal62.txt,https://github.com/REPO4EU/modulediscovery_test_data/blob/main/seedfiles/FTC-133.txt,https://github.com/REPO4EU/modulediscovery_test_data/blob/main/seedfiles/TPC-1.txt"
    network         = "https://github.com/REPO4EU/modulediscovery_test_data/blob/main/background_ppi/ppi_only_reviewed_proteins.csv"
    id_space        = "uniprot"
}
